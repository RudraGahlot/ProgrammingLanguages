package oppsInJava;

public class classExamples {

	public static void main(String[] args) {
		
	Box myBox = new Box();
	myBox.height = 12;
	myBox.width=15;
	myBox.depth = 13;
	double vol;
	vol = myBox.depth* myBox.width * myBox.height;
	
	 System.out.println(" the volume is = " + vol);
	
	}
}

class Box{
	double width;
	double height;
	double depth;	
}
