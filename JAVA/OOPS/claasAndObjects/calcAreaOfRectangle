package oppsInJava;

class Rectangle{
	int length;
	int breadth;
	void insert(int i, int j) {
		length = i;
		breadth =j;
	}
	void calculateArea() {System.out.println("Area of rectangle is ="+ length*breadth);}
}

public class calcRectangle {

	public static void main(String[] args) {
		Rectangle r1 = new Rectangle();
		Rectangle r2 = new Rectangle();
		
		r1.insert(11,6);
		r2.insert(12,5);
		
		r1.calculateArea();
		r2.calculateArea();

	}
}
